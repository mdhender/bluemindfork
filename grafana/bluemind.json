          "tooltip": {
            "value_type": "cumulative",
            "query_as_alias": true
          },
          "targets": [
            {
              "function": "mean",
              "column": "duration",
              "series": "hps.forward.timings",
              "query": "select  mean(\"duration\") as \"duration_mean\" from \"hps.forward.timings\" where  time > now() - 1h and location = '/webmail' group by time(1s)  order asc",
              "condition_filter": true,
              "condition_key": "location",
              "condition_op": "=",
              "condition_value": "'/webmail'",
              "alias": "webmail"
            },
            {
              "function": "mean",
              "column": "duration",
              "series": "hps.forward.timings",
              "query": "select  mean(\"duration\") as \"duration_mean\" from \"hps.forward.timings\" where  time > now() - 1h and location = '/settings' group by time(1s)  order asc",
              "condition_filter": true,
