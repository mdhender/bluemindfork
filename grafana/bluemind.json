              "series": "hps.forward.timings",
              "query": "select  mean(\"duration\") as \"duration_mean\" from \"hps.forward.timings\" where  time > now() - 1h and location = '/contact' group by time(1s)  order asc",
              "condition_filter": true,
              "condition_key": "location",
              "condition_op": "=",
              "condition_value": "'/contact'",
              "alias": "contact"
            },
            {
              "function": "mean",
              "column": "duration",
              "series": "hps.forward.timings",
              "query": "select  mean(\"duration\") as \"duration_mean\" from \"hps.forward.timings\" where  time > now() - 1h and location = '/cal' group by time(1s)  order asc",
