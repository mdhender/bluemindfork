<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.bluemind.core.rest.serviceFactory">
      <serviceFactory
            class="net.bluemind.device.service.DeviceServiceFactory">
      </serviceFactory>
      <serviceFactory
            class="net.bluemind.device.service.DevicesServiceFactory">
      </serviceFactory>
   </extension>
   <extension
         point="net.bluemind.core.rest.apiEndpoint">
      <endpoint
            api="net.bluemind.device.api.IDevice">
      </endpoint>
      <endpoint
            api="net.bluemind.device.api.IDeviceUids" 
            inline="true">
      </endpoint>
      <endpoint
            api="net.bluemind.device.api.IDevices">
      </endpoint>
   </extension>

</plugin>
