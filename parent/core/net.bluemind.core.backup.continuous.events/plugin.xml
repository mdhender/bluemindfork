      <hook
            class="net.bluemind.core.backup.continuous.events.UserAccountsContinuousHook">
      </hook>
   </extension>
   <extension
         point="net.bluemind.mailbox.identity.mailboxIdentityHook">
      <hook
            impl="net.bluemind.core.backup.continuous.events.MailboxIdentityContinuousHook">
      </hook>
   </extension>
   <extension
         point="net.bluemind.user.hook.usermailidentity">
      <hook 
      		impl="net.bluemind.core.backup.continuous.events.UserMailIdentitiesContinuousHook">
      </hook>
   </extension>
   <extension
         point="net.bluemind.scheduledjob.service.hook">
      <job_hook
            impl="net.bluemind.core.backup.continuous.events.JobPlanContinuousHook">
      </job_hook>
   </extension>
   <extension
         point="net.bluemind.backend.mail.replica.hook.messagebodyhook">
      <hook
            impl="net.bluemind.core.backup.continuous.events.MessageBodyHook"></hook>
      <hook
            impl="net.bluemind.core.backup.continuous.events.MessageBodyESSourceHook">
      </hook>
   </extension>
</plugin>
