<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.bluemind.system.hook">
      <observer
            class="net.bluemind.system.auth.KrbAuthConfigChangeHandler">
      </observer>
      <validator
            class="net.bluemind.system.auth.CasAuthConfigurationValidator">
      </validator>
      <validator
            class="net.bluemind.system.auth.HpsConfigChangeValidateHandler">
      </validator>
   </extension>
   <extension
         point="net.bluemind.domain.domainHook">
      <hook
            class="net.bluemind.system.auth.OpenIdConfigurationObserver">
      </hook>
   </extension>
   
</plugin>
