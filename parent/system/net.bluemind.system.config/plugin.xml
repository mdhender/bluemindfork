<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="net.bluemind.nginx.update" name="nginx-update" schema="schema/net.bluemind.nginx.update.exsd"/>
  <extension
         point="net.bluemind.system.hook">
      <observer
            class="net.bluemind.system.config.ConfigHook">
      </observer>
   </extension>
  <extension
         point="net.bluemind.system.hook">
      <observer
            class="net.bluemind.system.config.NginxExternalUrlConfigurationObserver">
      </observer>
   </extension>
  <extension
        point="net.bluemind.server.hook.serverhook">
     <hook
           impl="net.bluemind.system.config.NginxExternalUrlConfigurationOnServerTagged">
     </hook>
  </extension>
</plugin>
