<?xml version="1.0" encoding="utf-8"?>
<forwards>
	<forward path="/webmail" auth="true"
		target="locator://bm/webmail:81/webmail" role="hasMail">
		<whitelist>
			<wl uri="/skins/bluemind/images/favicon.ico" />
			<wl regex="^/webmail/css/(.*)$" />
			<wl regex="^/webmail/js/(.*)$" />
			<wl regex="^/webmail/style/(.*)$" />
			<wl regex="^/webmail/static/(.*)$" />
			<wl regex="^/webmail/images/(.*)$" />
			<wl regex="^/webmail/fonts/(.*)$" />
		</whitelist>
	</forward>
</forwards>
