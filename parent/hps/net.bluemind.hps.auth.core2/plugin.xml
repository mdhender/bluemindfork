<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.bluemind.proxy.http.authprovider">
      <auth_provider
            implementation="net.bluemind.hps.auth.core2.C2ProviderFactory">
      </auth_provider>
   </extension>
   <extension
         point="net.bluemind.hornetq.forwardToVertx">
      <vertx-forwarder
            class="net.bluemind.hps.auth.core2.ForwardSessionMessages">
      </vertx-forwarder>
   </extension>
   <extension
         point="net.bluemind.core.caches.registry.registration">
      <reg
            impl="net.bluemind.hps.auth.core2.C2ProviderFactory$CacheRegistration">
      </reg>
   </extension>
</plugin>
