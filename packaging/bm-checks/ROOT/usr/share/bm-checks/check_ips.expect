set username "admin0"
set password [exec cat /etc/bm/bm-core.tok]

log_user 0
set timeout 5
spawn telnet 127.0.0.1 144
expect {
    default {
        send_user "\[ERROR\] Failed to get IPS banner\n"
        exit 2
    }
    "* OK " {}
}

send "A1 LOGIN $username $password\n"
expect {
    default {
        send_user "\[ERROR\] IPS authentication fail!\n"
        exit 2
    }
    "A1 OK " {
        exit 0
    }
}
