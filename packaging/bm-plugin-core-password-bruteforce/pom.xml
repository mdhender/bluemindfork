<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<artifactId>bm-plugin-core-password-bruteforce</artifactId>
	<packaging>bm-service-extension</packaging>
	<properties>
		<service.name>bm-core</service.name>
		<shortDescription>BlueMind password bruteforce checker</shortDescription>
		<description>BlueMind password anti-bruteforce plugin.
		Disables user authentication if too many password failures occur within a small range of time</description>
	</properties>
	<parent>
		<groupId>net.bluemind</groupId>
		<artifactId>net.bluemind.packaging</artifactId>
		<version>4.1.0-SNAPSHOT</version>
	</parent>

	<dependencies>
		<dependency>
			<groupId>net.bluemind</groupId>
			<artifactId>net.bluemind.p2.ce.password-bruteforce</artifactId>
			<version>${project.version}</version>
			<type>zip</type>
		</dependency>
	</dependencies>
</project>

