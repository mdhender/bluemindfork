#!/usr/bin/make -f

include /usr/share/cdbs/1/rules/debhelper.mk

JAVA_HOME := /usr/lib/jvm/bm-jdk
# Do not generate crappy dependencies for unused libs
DEB_DH_SHLIBDEPS_ARGS_bm-plugin-core-forest := "-Xlib"

DEB_DH_STRIP_NONDETERMINISM_ARGS := "--exclude=."

build:
	echo "build"

install/bm-plugin-core-forest::
	cp -rf ${CURDIR}/ROOT/* ${CURDIR}/debian/bm-plugin-core-forest

