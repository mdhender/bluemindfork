msgid "plugins/net.bluemind.core.jobs/templates/EventSubjectAlert"
msgstr ""
"<@compress single_line=true>\n"
"Reminder: ${title} starts in ${reminder_duration}\n"
"<#if reminder_unit == \"seconds\">\n"
"  <#if reminder_duration == 1>\n"
"    second\n"
"  <#else>\n"
"    seconds\n"
"  </#if>\n"
"<#elseif reminder_unit == \"minutes\">\n"
"  <#if reminder_duration == 1>\n"
"    minute\n"
"  <#else>\n"
"    minutes\n"
"  </#if>\n"
"<#elseif reminder_unit == \"hours\">\n"
"  <#if reminder_duration == 1>\n"
"    hour\n"
"  <#else>\n"
"    hours\n"
"  </#if>\n"
"<#else>\n"
"  <#if reminder_duration == 1>\n"
"    day\n"
"  <#else>\n"
"    days\n"
"  </#if>\n"
"</#if>\n"
"</@compress>"

msgid "plugins/net.bluemind.core.jobs/templates/EventDetail"
msgstr ""
"<table>\n"
"  <tr>\n"
"    <td colspan=\"2\"><h2>${title} <#if .data_model.old_title??><#if old_title != title><i class=\"updated\">(updated)</i></#if></#if></h2></td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td class=\"key\">When</td>\n"
"    <td class=\"value\">\n"
"      <#if allday == \"true\">\n"
"        <#if datebegin?string(date_format) == dateend?string(date_format)>\n"
"          on <span class=\"date\">${datebegin?string(date_format)}</span>\n"
"        <#else>\n"
"          from <span class=\"date\">${datebegin?string(date_format)}</span><br />\n"
"          to <span class=\"date\">${dateend?string(date_format)}</span>\n"
"        </#if>\n"
"      <#else>\n"
"        <#if datebegin?string(date_format) == dateend?string(date_format)>\n"
"          on <span class=\"date\">${datebegin?string(date_format)}</span> <br />\n"
"          from <span class=\"date\">${datebegin?string(time_format)}</span> to <span class=\"date\">${dateend?string(time_format)}</span>\n"
"          <#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"        <#else>\n"
"          from <span class=\"date\">${datebegin?string(date_format)}, ${datebegin?string(time_format)}</span>\n"
"          <#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"          <br />\n"
"          to <span class=\"date\">${dateend?string(date_format)}, ${dateend?string(time_format)}</span>\n"
"          <#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"        </#if> \n"
"        <#if .data_model.old_date??><#if old_date?string(datetime_format) != date?string(datetime_format) || old_duration != duration><i class=\"updated\">(mis à jour)</i></#if></#if>\n"
"      </#if>\n"
"    </td>\n"
"  </tr>\n"
"  <#if recurrenceKind??>\n"
"  <#assign seq = [\"sunday\", \"monday\", \"tuesday\", \"wednesday\", \"thrusday\", \"friday\", \"saturday\"]>\n"
"  <tr>\n"
"    <td class=\"key\"></td>\n"
"    <td class=\"value\">\n"
"    <#if recurrenceFreq == 1>\n"
"\t    <#switch recurrenceKind>\n"
"\t      <#case \"daily\">\n"
"\t        daily\n"
"\t      <#break>\n"
"\t      <#case \"weekly\">\n"
"\t        Weekly, on \n"
"\t        <#list recurrenceDays as x>\n"
"\t          <#if x == \"1\">\n"
"\t            ${seq[x_index]}\n"
"\t          </#if>\n"
"\t\t\t</#list>  \n"
"\t      <#break>\n"
"\t      <#case \"monthlybydate\">\n"
"\t\t  <#case \"monthlybyday\">\n"
"\t        Monthly\n"
"\t      <#break>\n"
"\t      <#case \"yearly\">\n"
"\t        Yearly\n"
"\t      <#break>\n"
"\t    </#switch>\n"
"    <#else>\n"
"\t\t<#switch recurrenceKind>\n"
"\t      <#case \"daily\">\n"
"\t        Every ${recurrenceFreq} days\n"
"\t      <#break>\n"
"\t      <#case \"weekly\">\n"
"\t        Every ${recurrenceFreq} weeks\n"
"\t      <#break>\n"
"\t      <#case \"monthlybydate\">\n"
"\t\t  <#case \"monthlybyday\">\n"
"\t        Every ${recurrenceFreq} months\n"
"\t      <#break>\n"
"\t      <#case \"yearly\">\n"
"\t        Every ${recurrenceFreq} years\n"
"\t      <#break>\n"
"\t    </#switch>\n"
"    </#if>\n"
"    </td>\n"
"  </tr>\n"
"  </#if>\n"
"  <#if location??>\n"
"    <tr>\n"
"      <td class=\"key\">Where</td>\n"
"      <td class=\"value\">${location} <#if .data_model.old_location??><#if old_location != location><i class=\"updated\">(updated)</i></#if></#if></td>\n"
"    </tr>\n"
"  </#if>\n"
"  <tr>\n"
"    <td class=\"key\">Organizer</td><td class=\"value\">${owner}</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td class=\"key\">Attendees</td>\n"
"    <td class=\"value\">\n"
"      <ul class=\"attendees\">\n"
"        <#list attendees as attendee>\n"
"          <li>${attendee}</li>\n"
"        </#list>\n"
"      </ul>\n"
"    </td>\n"
"  </tr>\n"
"  <#if description??>\n"
"    <tr>\n"
"      <td class=\"key\">Description</td>\n"
"      <td class=\"value\">${description} <#if .data_model.old_description??><#if old_description != description><i class=\"updated\">(updated)</i></#if></#if></td>\n"
"    </tr>\n"
"  </#if>\n"
"  <tr>\n"
"    <td colspan=\"2\"><h2>&nbsp;</h2></td>\n"
"  </tr>\n"
"</table>\n"

