msgid "plugins/net.bluemind.core.jobs/templates/EventDetail"
msgstr ""
"<table>\n"
"  <tr>\n"
"    <td colspan=\"2\"><h2>${title} <#if .data_model.old_title??><#if "
"old_title != title><i class=\"updated\">(frissítve)</i></#if></#if></h2></td>"
"\n"
"  </tr>\n"
"  <tr>\n"
"    <td class=\"key\">Amikor</td>\n"
"    <td class=\"value\">\n"
"      <#if allday == \"true\">\n"
"        <#if datebegin?string(date_format) == dateend?string(date_format)>\n"
"          ekkor: <span class=\"date\">${datebegin?string(date_format)}</span>"
"\n"
"        <#else>\n"
"          ettől: <span class=\"date\""
">${datebegin?string(date_format)}</span><br />\n"
"          eddig: <span class=\"date\">${dateend?string(date_format)}</span>\n"
"        </#if>\n"
"      <#else>\n"
"        <#if datebegin?string(date_format) == dateend?string(date_format)>\n"
"          ekkor <span class=\"date\">${datebegin?string(date_format)}</span> "
"<br />\n"
"          ettől: <span class=\"date\">${datebegin?string(time_format)}</span>"
" eddig: <span class=\"date\">${dateend?string(time_format)}</span>\n"
"          <#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"        <#else>\n"
"          ettől:<#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"          <br />\n"
"          eddig: <span class=\"date\">${dateend?string(date_format)}, "
"${dateend?string(time_format)}</span>\n"
"          <#if tz??>\n"
"            <span class=\"tz\">(${tz})</span>\n"
"          </#if>\n"
"        </#if> \n"
"        <#if .data_model.old_date??><#if old_date?string(datetime_format) != "
"date?string(datetime_format) || old_duration != duration><i class=\"updated\""
">(frissítve)</i></#if></#if>\n"
"      </#if>\n"
"    </td>\n"
"  </tr>\n"
"  <#if recurrenceKind??>\n"
"  <#assign seq = [\"vasárnap\", \"hétfő\", \"kedd\", \"szerda\", \""
"csütörtök\", \"péntek\", \"szombat\"]>\n"
"  <tr>\n"
"    <td class=\"key\"></td>\n"
"    <td class=\"value\">\n"
"    <#if recurrenceFreq == 1>\n"
"\t    <#switch recurrenceKind>\n"
"\t      <#case \"daily\">\n"
"\t        naponta\n"
"\t      <#break>\n"
"\t      <#case \"weekly\">\n"
"\t        Hetente, ekkor: \n"
"\t        <#list recurrenceDays as x>\n"
"\t          <#if x == \"1\">\n"
"\t            ${seq[x_index]}\n"
"\t          </#if>\n"
"\t\t\t</#list>  \n"
"\t      <#break>\n"
"\t      <#case \"monthlybydate\">\n"
"\t\t  <#case \"monthlybyday\">\n"
"\t        Havonta\n"
"\t      <#break>\n"
"\t      <#case \"yearly\">\n"
"\t        Évente\n"
"\t      <#break>\n"
"\t    </#switch>\n"
"    <#else>\n"
"\t\t<#switch recurrenceKind>\n"
"\t      <#case \"daily\">\n"
"\t        Minden ${recurrenceFreq} nap\n"
"\t      <#break>\n"
"\t      <#case \"weekly\">\n"
"\t        Minden ${recurrenceFreq} hét\n"
"\t      <#break>\n"
"\t      <#case \"monthlybydate\">\n"
"\t\t  <#case \"monthlybyday\">\n"
"\t        Minden ${recurrenceFreq} hónap\n"
"\t      <#break>\n"
"\t      <#case \"yearly\">\n"
"\t        Minden ${recurrenceFreq} év\n"
"\t      <#break>\n"
"\t    </#switch>\n"
"    </#if>\n"
"    </td>\n"
"  </tr>\n"
"  </#if>\n"
"  <#if location??>\n"
"    <tr>\n"
"      <td class=\"key\">Ahol</td>\n"
"      <td class=\"value\">${location} <#if .data_model.old_location??><#if "
"old_location != location><i class=\"updated\""
">(frissítve)</i></#if></#if></td>\n"
"    </tr>\n"
"  </#if>\n"
"  <tr>\n"
"    <td class=\"key\">Szervező</td><td class=\"value\">${owner}</td>\n"
"  </tr>\n"
"  <tr>\n"
"    <td class=\"key\">Résztvevők</td>\n"
"    <td class=\"value\">\n"
"      <ul class=\"attendees\">\n"
"        <#list attendees as attendee>\n"
"          <li>${attendee}</li>\n"
"        </#list>\n"
"      </ul>\n"
"    </td>\n"
"  </tr>\n"
"  <#if description??>\n"
"    <tr>\n"
"      <td class=\"key\">Leírás</td>\n"
"      <td class=\"value\">${description} <#if "
".data_model.old_description??><#if old_description != description><i class=\""
"updated\">(frissítve)</i></#if></#if></td>\n"
"    </tr>\n"
"  </#if>\n"
"  <tr>\n"
"    <td colspan=\"2\"><h2>&nbsp;</h2></td>\n"
"  </tr>\n"
"</table>\n"

