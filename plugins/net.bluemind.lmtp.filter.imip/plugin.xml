<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.bluemind.lmtp.lmtpfilterfactory">
      <lmtp_filter_factory
            implementation="net.bluemind.lmtp.filter.imip.FilterFactory"></lmtp_filter_factory>
   </extension>
   <extension
         point="net.bluemind.hornetq.forwardToVertx">
      <vertx-forwarder
            class="net.bluemind.lmtp.filter.imip.cache.MailboxMessageForwarder">
      </vertx-forwarder>
   </extension>
   <extension
         point="net.bluemind.lib.vertx.verticles">
      <verticle
            impl="net.bluemind.lmtp.filter.imip.cache.MailboxCacheFactory">
      </verticle>
   </extension>

</plugin>
