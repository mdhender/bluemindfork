<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateLocales='default' ui:useSafeHtmlTemplates='true'
	xmlns:bm="urn:import:net.bluemind.ui.im.client">

	<g:FlowPanel>

		<bm:Photo ui:field="photo" />

		<g:FlowPanel ui:field="infos">
			<g:Label ui:field="displayName" />
			<g:FlowPanel ui:field="statusContainer">
				<g:Label ui:field="statusPhone" visible="false"></g:Label>
				<g:SimplePanel ui:field="statusIM" />
			</g:FlowPanel>
			<g:MenuBar ui:field="statusMenu">
				<g:MenuItem ui:field="statusMenuItem" />
			</g:MenuBar>
		</g:FlowPanel>


	</g:FlowPanel>

</ui:UiBinder>