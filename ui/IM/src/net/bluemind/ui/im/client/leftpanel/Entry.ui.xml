<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateLocales='default' ui:useSafeHtmlTemplates='true'
	xmlns:bm="urn:import:net.bluemind.ui.im.client">

	<g:FlowPanel>
		<g:SimplePanel ui:field="photoContainer">
			<bm:Photo ui:field="photo" />
		</g:SimplePanel>

		<g:FlowPanel ui:field="infos" width="160px">
			<g:Label ui:field="displayName" />
			<g:Label ui:field="statusPhone" visible="false"></g:Label>
			<g:SimplePanel ui:field="statusIM" />
			<g:Label ui:field="statusLabel" />
			<g:Label ui:field="resendSubscriptionRequest" visible="false"></g:Label>
		</g:FlowPanel>
		<g:Label ui:field="subscriptionAction" />

	</g:FlowPanel>

</ui:UiBinder>