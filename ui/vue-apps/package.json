        "vue": "^2.6.14"
    },
    "scripts": {
        "test": "NODE_ICU_DATA=node_modules/full-icu jest --config node_modules/@bluemind/conf/jest.config.js",
        "root": "cd root-webapp && yarn dev",
        "mail": "cd mail-webapp && yarn dev",
        "videoconferencing-saas": "cd videoconferencing-saas-webapp && yarn dev",
	"filehosting-plugin": "cd webmodules/net.bluemind.webmodules.filehosting && yarn dev",
        "test-ci": "NODE_ICU_DATA=node_modules/full-icu jest --ci --json --config node_modules/@bluemind/conf/jest.config.js --testResultsProcessor jest-jenkins-reporter --coverage --coverageReporters cobertura > jest.json"
    },
    "prettier": "@bluemind/prettier-config",
    "browserslist": [
        "> 0.25%",
        "not dead",
        "not ie <= 8"
