{
    "private": true,
    "workspaces": {
        "packages": [
            "commons/*",
            "mailbackend/*",
            "mail-webapp",
            "root-webapp",
            "videoconferencing-saas-webapp",
            "webapp-wrapper", 
            "webmodules/net.bluemind.webmodules.filehosting"
        ],
