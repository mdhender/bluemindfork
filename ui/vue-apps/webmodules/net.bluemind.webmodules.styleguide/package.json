{
    "name": "@bluemind/webmodules.styleguide",
    "description": "Bluemind webmodule to provide one instance of BlueMind Styleguide",
    "version": "4.1.0",
    "license": "AGPL-3.0-only",
    "author": "Blue Mind SAS",
    "scripts": {
        "build": "npm-run-all -s clean copy:*",
        "clean": "rm -fr ./web-resources/* && mkdir -p ./web-resources/js/ && mkdir -p ./web-resources/css/",
        "copy:js": "cp ../../node_modules/@bluemind/ui-components/dist/ui-components.js ./web-resources/js/styleguide.js",
        "copy:js-map": "cp ../../node_modules/@bluemind/ui-components/dist/ui-components.js.map ./web-resources/js/styleguide.js.map",
        "copy:css": "cp ../../node_modules/@bluemind/ui-components/dist/ui-components.css ./web-resources/css/styleguide.css"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "@bluemind/ui-components": "^4.1.0"
    }
}
