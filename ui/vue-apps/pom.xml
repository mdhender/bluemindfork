<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<artifactId>net.bluemind.vue-apps</artifactId>
	<version>4.1.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>net.bluemind</groupId>
		<artifactId>net.bluemind.global</artifactId>
		<version>4.1.0-SNAPSHOT</version>
		<relativePath>../../global/pom.xml</relativePath>
	</parent>
	<properties>
		<!-- Set this build number to the last locked webmail build. It is used by CI "bm-vue-apps" when param BM_VERSION is not supplied. -->
		<bm-runtime.url>https://forge.bluemind.net/staging/p2/bluemind/4.1.51056/</bm-runtime.url>
	</properties>
	<repositories>
		<repository>
			<id>bluemind</id>
			<layout>p2</layout>
			<url>${bm-runtime.url}</url>
		</repository>
	</repositories>
	<modules>
		<module>mail-webapp</module>
		<module>root-webapp</module>
		<module>videoconferencing-saas-webapp</module>
		<module>webapp-wrapper</module>
		<module>webmodules</module>
	</modules>
	<profiles>
		<profile>
			<id>open-packaging</id>
			<modules>
				<module>packaging</module>
			</modules>
		</profile>
	</profiles>
</project>
