{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "downlevelIteration": true,
        "target": "ES2021",
        "module": "commonjs",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "onlyCompileBundledFiles": true
}
