<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateLocales='default' ui:useSafeHtmlTemplates='true'>


	<ui:style>
		.progressBarContainer {
			padding: 10px;
			height: 30px;
		}
	</ui:style>


	<g:HTMLPanel>
			<g:SimplePanel styleName="{style.progressBarContainer}">
				<g:SimplePanel styleName="progress progress-striped active">
					<g:SimplePanel ui:field="progress" styleName="bar"><g:Label visible="false" ui:field="progressText"></g:Label></g:SimplePanel>
				</g:SimplePanel>
			</g:SimplePanel>
	</g:HTMLPanel>

</ui:UiBinder>
