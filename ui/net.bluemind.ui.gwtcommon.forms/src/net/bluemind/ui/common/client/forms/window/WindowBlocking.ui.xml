<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:DockLayoutPanel unit="PX" styleName="modalPanel"
		height="150px" ui:field="dlp">

		<g:north size="40">
			<g:Label ui:field="headerMsg" styleName="modalHeader" />
		</g:north>

		<g:center>
			<g:FlowPanel styleName="modalContent">
				<g:HTMLPanel>
					<g:HTML ui:field="contentMsg"/>
				</g:HTMLPanel>
			</g:FlowPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>
