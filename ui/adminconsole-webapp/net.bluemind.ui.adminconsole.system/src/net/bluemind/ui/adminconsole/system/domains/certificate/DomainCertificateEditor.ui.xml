<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateLocales='default' ui:useSafeHtmlTemplates='true'
	xmlns:f="urn:import:net.bluemind.ui.adminconsole.base.ui"
	xmlns:my="urn:import:net.bluemind.ui.adminconsole.security.certificate">

	<ui:with field="c"
		type="net.bluemind.ui.adminconsole.security.certificate.l10n.CertificateEditorConstants" />
	<ui:with field="d"
		type="net.bluemind.ui.adminconsole.system.domains.l10n.DomainConstants" />

	<g:HTMLPanel>
		<f:FieldSetPanel name="{d.domainCertificate}" />
		<table border="0">
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.domainUid}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:Label width="400px" ui:field="domainUid" />
				</td>
			</tr>
		</table>
		<my:CertificateEditorComponent ui:field="certificateData" />
	</g:HTMLPanel>
</ui:UiBinder>