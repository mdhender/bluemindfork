				<td style='width:400px'>
					<g:ListBox multipleSelect="false" ui:field="defaultAppRedirector"></g:ListBox>
				</td>
			</tr>
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.reverseProxy}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:ListBox multipleSelect="false" ui:field="reverseProxy"></g:ListBox>
				</td>
			</tr>
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.database}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:ListBox multipleSelect="false" ui:field="database"></g:ListBox>
				</td>
			</tr>
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.fileHostingServer}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:ListBox multipleSelect="false" ui:field="filehosting"></g:ListBox>
