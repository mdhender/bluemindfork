<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:bj="urn:import:net.bluemind.ui.admin.client" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateLocales='default' ui:useSafeHtmlTemplates='true'
	xmlns:f="urn:import:net.bluemind.ui.adminconsole.base.ui" xmlns:common="urn:import:net.bluemind.ui.common.client.forms">

	<ui:with field="c"
		type="net.bluemind.ui.adminconsole.system.hosts.l10n.HostConstants" />


	<g:HTMLPanel>
		<f:FieldSetPanel name="{c.generalTab}" />
		<table border="0">
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.name}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:TextBox width="400px" ui:field="name" />
				</td>
			</tr>
			
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.ip}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:TextBox width="400px" ui:field="ip" />
				</td>
			</tr>
			
			<tr>
				<td style='width:200px'>
					<g:Label text="{c.fqdn}:"></g:Label>
				</td>
				<td style='width:400px'>
					<g:TextBox width="400px" ui:field="fqdn" />
				</td>
			</tr>
		</table>

	</g:HTMLPanel>

</ui:UiBinder>