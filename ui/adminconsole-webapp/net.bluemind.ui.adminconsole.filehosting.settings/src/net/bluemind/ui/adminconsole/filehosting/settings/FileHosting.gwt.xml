<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="net.bluemind.ui.adminconsole.filehosting.settings">
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.xml.XML" />
	<inherits name="com.google.gwt.json.JSON"/>
	
	<inherits name="net.bluemind.ui.adminconsole.AdminConsoleBase" />
	<inherits name="net.bluemind.ui.adminconsole.system.System"/>
	
	<inherits name="net.bluemind.ui.common.CommonIcons" />
	<inherits name="net.bluemind.ui.common.client.CommonForms" />
	
	<inherits name="net.bluemind.ui.extensions.GWTExtensions" />
	
	<inherits name="net.bluemind.core.CommonsApi" />
	<inherits name="net.bluemind.core.CoreApi" />
	
	<inherits name="net.bluemind.gwtconsoleapp.base.Notification" />
	<inherits name="net.bluemind.gwtconsoleapp.base.Eventbus" />
	
	<inherits name="net.bluemind.gwtconsoleapp.base.Editor" />
	
	<inherits name="net.bluemind.filehosting.FilehostingApi"/>
		<!-- Specify the app entry point class. -->
	<entry-point class='net.bluemind.ui.adminconsole.filehosting.settings.FileHostingPlugin' />
	
	<source path='' />
	
	<!-- Internationalization support. -->
	<extend-property name="locale" values="en" />
	<extend-property name="locale" values="fr" />
	<set-property-fallback name="locale" value="en" />

	<add-linker name="xsiframe" />
	<collapse-all-properties />

</module>
