<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='net.bluemind.ui.settings'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='net.bluemind.ui.settings.Settings' />
	<!-- Specify the paths for translatable code -->
	<source path='client' />

	<!-- Internationalization support. -->
	<extend-property name="locale" values="en" />
 	<extend-property name="locale" values="fr" />
 	<extend-property name="locale" values="it" />
	<extend-property name="locale" values="sk" />
	<extend-property name="locale" values="es" />
	<extend-property name="locale" values="pl" />
	<extend-property name="locale" values="de" />
	<extend-property name="locale" values="it" />
	<extend-property name="locale" values="zh" />
	<extend-property name="locale" values="hu" />
	<set-property-fallback name="locale" value="en" />
</module>
