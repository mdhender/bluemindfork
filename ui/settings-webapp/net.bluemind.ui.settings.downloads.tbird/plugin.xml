<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.bluemind.ui.settings.downloads">
      <download
            description="%download.description.0"
            role="hasTbird"
            section="downloads"
            title="%download.title.0"
            url="/settings/settings/download/tbird-webext.xpi">
      </download>
   </extension>
   <extension
         point="net.bluemind.webmodule">
      <web-module-provider
            module="/settings">
         <web-resource
               bundle="net.bluemind.ui.settings.downloads.tbird">
         </web-resource>
         <handler
               class="net.bluemind.ui.settings.downloads.tbird.TBirdDownloadHandlerWebExt"
               path="settings/download/tbird-webext.xpi">
         </handler>
         <handler
               class="net.bluemind.ui.settings.downloads.tbird.TBirdUpdateJsonWebExt"
               path="settings/download/updateJson-webext">
         </handler>
      </web-module-provider>
   </extension>
</plugin>
