      <plugin id="net.bluemind.extensions.closure"/>
      <plugin id="net.bluemind.lib.freemarker"/>
      <plugin id="net.bluemind.lib.guava"/>
      <plugin id="net.bluemind.lib.jackson"/>
      <plugin id="net.bluemind.lib.vertx"/>
      <plugin id="net.bluemind.slf4j"/>
      <plugin id="net.bluemind.ui.defaultapp"/>
      <plugin id="net.bluemind.webmodule"/>
      <plugin id="net.bluemind.webmodule.server"/>
      <plugin id="net.bluemind.webmodules.launcher"/>
      <plugin id="org.eclipse.core.contenttype"/>
      <plugin id="org.eclipse.core.jobs"/>
      <plugin id="org.eclipse.core.runtime"/>
