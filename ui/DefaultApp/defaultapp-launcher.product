<?xml version="1.0" encoding="UTF-8"?>
<?pde version="3.5"?>

<product name="defaultapp" id="net.bluemind.ui.defaultapp.defaultapp" application="net.bluemind.webmodules.launcher.webLauncher" useFeatures="false" includeLaunchers="true">

   <configIni use="default">
   </configIni>

   <launcherArgs>
      <vmArgs>-Dosgi.noShutdown=true -Duser.timezone=GMT -Djava.security.egd=file:/dev/urandom -Djava.awt.headless=true -Xmx2048m
      </vmArgs>
      <vmArgsMac>-XstartOnFirstThread -Dorg.eclipse.swt.internal.carbon.smallFonts
      </vmArgsMac>
   </launcherArgs>

   <launcher name="defaultapp">
      <solaris/>
